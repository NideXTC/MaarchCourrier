<?xml version="1.0" encoding="utf-8"?>
<ROOT>
    <mail_natures>
        <nature with_reference="false">
            <id>simple_mail</id>
            <label>_SIMPLE_MAIL</label>
        </nature>
        <nature with_reference="false">
            <id>email</id>
            <label>_EMAIL</label>
        </nature>
        <nature with_reference="false">
            <id>fax</id>
            <label>_FAX</label>
        </nature>
        <nature with_reference="true">
            <id>chronopost</id>
            <label>_CHRONOPOST</label>
        </nature>
        <nature with_reference="true">
            <id>fedex</id>
            <label>_FEDEX</label>
        </nature>
        <nature with_reference="true">
            <id>registered_mail</id>
            <label>_REGISTERED_MAIL</label>
        </nature>
        <nature with_reference="false">
            <id>courier</id>
            <label>_COURIER</label>
        </nature>
        <nature with_reference="false">
            <id>other</id>
            <label>_OTHER</label>
        </nature>
        <default_nature>simple_mail</default_nature>
    </mail_natures>
    <attachment_types>
        <type show="true" with_chrono="true" attach_in_mail="true">
            <id>response_project</id>
            <label>_RESPONSE_PROJECT</label>
            <processing_mode></processing_mode>
        </type>
        <type show="true" with_chrono="true">
            <id>transmission</id>
            <label>Transmission</label>
            <processing_mode></processing_mode>
        </type>
		<type show="false" with_chrono="true" attach_in_mail="true">
            <id>signed_response</id>
            <label>_SIGNED_RESPONSE</label>
            <processing_mode></processing_mode>
        </type>
        <type show="true" with_chrono="true">
            <id>outgoing_mail_signed</id>
            <label>_OUTGOING_MAIL_SIGNED</label>
            <processing_mode></processing_mode>
        </type>
        <type show="true" with_chrono="false">
            <id>simple_attachment</id>
            <label>_A_PJ</label>
            <processing_mode></processing_mode>
        </type>
        <type show="true" with_chrono="true" get_chrono="response_project">
            <id>waybill</id>
            <label>_WAYBILL</label>
            <processing_mode></processing_mode>
        </type>
	<type show="false" with_chrono="true">
            <id>routing</id>
            <label>_ROUTING</label>
            <processing_mode></processing_mode>
        </type>
		<type show="true" with_chrono="true">
            <id>outgoing_mail</id>
            <label>_OUTGOING_MAIL</label>
            <processing_mode></processing_mode>
        </type>
	<type show="false" with_chrono="false">
            <id>converted_pdf</id>
            <label>_CONVERTED_PDF</label>
            <processing_mode></processing_mode>
        </type>
	<type show="false" with_chrono="false">
            <id>print_folder</id>
            <label>_PRINT_FOLDER</label>
            <processing_mode></processing_mode>
        </type>
        <type show="true" with_chrono="false">
            <id>sva</id>
            <label>_SVA</label>
            <processing_mode>SVA</processing_mode>
        </type>
        <type show="true" with_chrono="false">
            <id>svr</id>
            <label>_SVR</label>
            <processing_mode>SVR</processing_mode>
        </type>
    </attachment_types>
    <processing_modes>
        <processing_mode>
            <label>NORMAL</label>
        </processing_mode>
        <processing_mode>
            <label>SVA</label>
        </processing_mode>
        <processing_mode>
            <label>SVR</label>
        </processing_mode>
    </processing_modes>
    <priorities>
        <!-- Tag working_days "true" = Jours ouvrés // "false" = Jours calendaires -->
        <priority with_delay="4" working_days="true">Très urgent</priority> <!-- 0 -->
        <priority with_delay="8" working_days="true">Urgent</priority> <!-- 1 -->
        <priority with_delay="false" working_days="true">Normal</priority> <!-- 2 -->
        <priority with_delay="false" working_days="true">_LOW</priority> <!-- 3 -->
        <priority with_delay="false" working_days="true">SVA/SVR</priority> <!-- 4 -->
        <!--<priority with_delay="false" working_days="true">_VERY_LOW</priority>  4 -->
        <default_priority with_delay="false" working_days="true">2</default_priority>
        <default_sve_priority with_delay="false" working_days="true">4</default_sve_priority>
    </priorities>
    <contact_check>
        <check_days_before>1</check_days_before> <!-- 0 if you want to disabled -->
    </contact_check>
    <titles>
        <title>
            <id>title1</id>
            <label>Monsieur</label>
        </title>
        <title>
            <id>title2</id>
            <label>Madame</label>
        </title>
        <title>
            <id>title3</id>
            <label>Mademoiselle</label>
        </title>
        <title>
            <id>title4</id>
            <label>Avocat</label>
        </title>
        <title>
            <id>title5</id>
            <label>Notaire</label>
        </title>
        <title>
            <id>title6</id>
            <label>Mairie</label>
        </title>
        <title>
            <id>title7</id>
            <label>Ecole</label>
        </title>
        <title>
            <id>title8</id>
            <label>Madame et Monsieur</label>
        </title>
        <title>
            <id>title9</id>
            <label>Messieurs</label>
        </title>
        <default_title/>
    </titles>
</ROOT>
